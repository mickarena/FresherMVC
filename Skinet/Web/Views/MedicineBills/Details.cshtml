@model Core.Entities.MedicineBill

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>MedicineBill</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Doctor ID
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DoctorID)
        </dd>
        <dt class = "col-sm-2">
            Created date
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DateCreate)
        </dd>
        <dt class = "col-sm-2">
            Purchase status
        </dt>
        <dd class = "col-sm-10">
            @(Model.PayStatus == false ? "No" : "Yes")
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
